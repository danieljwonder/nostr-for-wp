{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "nostr-for-wp/note",
  "title": "Nostr Note",
  "category": "widgets",
  "icon": "format-chat",
  "description": "Display a single Nostr note.",
  "keywords": ["nostr", "note", "social"],
  "textdomain": "nostr-for-wp",
  "attributes": {
    "noteId": {
      "type": "number",
      "default": 0
    }
  },
  "supports": {
    "html": false
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css"
}

